{
  "name": "vchat",
  "version": "0.9.3",
  "description": "An experimental video chat server/client hybrid",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember nw",
    "build": "ember nw:package",
    "test": "ember nw:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdconaway/vchat.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Michael Conaway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdconaway/vchat/issues"
  },
  "homepage": "https://github.com/mdconaway/vchat#readme",
  "dependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-ajax": "^2.4.1",
    "ember-cli": "2.12.1",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-babel": "^5.1.7",
    "ember-browserify": "^1.1.13",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-nwjs": "^0.3.9",
    "ember-cli-qunit": "^3.1.0",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.12.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-source": "~2.12.0",
    "ember-welcome-page": "^2.0.2",
    "external-ip": "^0.2.3",
    "graceful-fs": "^4.1.11",
    "loader.js": "^4.2.3",
    "node-uuid": "^1.4.3",
    "nw": "0.21.4",
    "pem": "^1.7.2",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "uri-js": "^3.0.2"
  },
  "main": "dist/index.html",
  "window": {
    "width": 960,
    "height": 600,
    "min_width": 960,
    "min_height": 600,
    "position": "center",
    "transparent": false,
    "frame": true
  },
  "keywords": [
    "ember",
    "NW",
    "NW.js",
    "NWjs",
    "video",
    "chat",
    "WebRTC",
    "STUN"
  ],
  "chromium-args": "--ignore-certificate-errors --enable-webgl --ignore-gpu-blacklist --auth-schemes='digest,ntlm,negotiate,basic'",
  "webkit": {
    "plugin": false
  },
  "single-instance": false
}
