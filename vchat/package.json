{
  "name": "vchat",
  "version": "0.0.1",
  "description": "An experimental video chat server/client hybrid",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember nw",
    "build": "ember nw:package",
    "test": "ember nw:test"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "external-ip": "^0.2.3",
    "node-uuid": "^1.4.3",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "pem": "^1.7.2"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "broccoli-funnel": "^0.2.3",
    "broccoli-merge-trees": "^0.2.1",
    "ember-cli": "0.2.5",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-babel": "^5.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-htmlbars": "0.7.6",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-node-webkit": "0.3.3",
    "ember-cli-qunit": "0.3.13",
    "ember-cli-uglify": "^1.0.1",
    "ember-data": "1.0.0-beta.17",
    "ember-disable-proxy-controllers": "^0.7.0",
    "ember-export-application-global": "^1.0.2",
    "nw": "0.12.2"
  },
  "main": "dist/index.html",
  "window": {
    "width": 960,
    "height": 600,
    "position": "center",
    "transparent": false,
    "frame": true,
    "toolbar": true
  },
  "chromium-args": "--ignore-certificate-errors --enable-webgl --ignore-gpu-blacklist --auth-schemes='digest,ntlm,negotiate,basic'",
  "webkit": {"plugin": false },
  "single-instance": false
